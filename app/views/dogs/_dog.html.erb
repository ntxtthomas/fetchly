<div id="<%= dom_id dog %>">
  <p>
    <strong>Name:</strong>
    <%= dog.name %>
  </p>

  <p>
    <strong>Breed:</strong>
    <%= dog.breed %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= dog.age %>
  </p>

  <p>
    <strong>Weight:</strong>
    <%= dog.weight %>
  </p>

  <p>
    <strong>Owner:</strong>
    <%= dog.owner_id %>
  </p>

</div>
